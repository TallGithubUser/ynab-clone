<% if @transactions.length > 0 %>
	<ul> 
		<% @transactions.each do |t| %>
			<li> <%= t[:account_name]%>, <%= t[:date]%>, <%= t[:payee]%>, <%= t[:category_group]%>, <%= t[:memo]%>, <%= t[:outflow]%>, <%= t[:inflow]%>  </li>
		<% end %>
	</ul>
<% else %> 
	<img src="<%= asset_path( 'no_transactions.png' ) %>" />
<% end %>